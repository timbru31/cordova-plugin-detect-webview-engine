{
  "name": "cordova-plugin-detect-webview-engine",
  "version": "3.0.1",
  "description": "Apache Cordova plugin to determine whether the application is using UIWebView or WKWebView engine",
  "author": "Tim Brust <github@timbrust.de>",
  "license": "MIT",
  "repository": "timbru31/cordova-plugin-detect-webview-engine",
  "bugs": {
    "url": "https://github.com/timbru31/cordova-plugin-detect-webview-engine/issues"
  },
  "scripts": {
    "lint": "npm run lint:ios && npm run lint:typescript",
    "lint:ios": "swiftlint",
    "lint:typescript": "eslint .",
    "build": "rm -rf dist && tsc",
    "prerelease": "npm run build",
    "release": "commit-and-tag-version -s -a"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cordova": "^11.0.3",
    "commit-and-tag-version": "^12.6.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "replace": "^1.2.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "cordova": {
    "id": "cordova-plugin-detect-webview-engine",
    "platforms": [
      "ios"
    ]
  },
  "engines": {
    "cordovaDependencies": {
      "2.0.0": {
        "cordova": ">= 9.0.0",
        "cordova-ios": ">=5.0.0"
      },
      "1.0.1": {
        "cordova": ">= 6.4.0",
        "cordova-ios": ">=4.3.0"
      }
    }
  },
  "keywords": [
    "cordova",
    "cordova-plugin",
    "ecosystem:cordova",
    "cordova-ios",
    "engine",
    "webview",
    "wkwebview",
    "uiwebview"
  ],
  "funding": "https://github.com/sponsors/timbru31"
}
