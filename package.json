{
  "name": "cordova-plugin-detect-webview-engine",
  "version": "3.0.1",
  "description": "Apache Cordova plugin to determine whether the application is using UIWebView or WKWebView engine",
  "author": "Tim Brust <github@timbrust.de>",
  "license": "MIT",
  "repository": "timbru31/cordova-plugin-detect-webview-engine",
  "bugs": {
    "url": "https://github.com/timbru31/cordova-plugin-detect-webview-engine/issues"
  },
  "scripts": {
    "lint": "tslint -c tslint.json 'www/**/*.ts' && swiftlint",
    "build": "rm -rf dist && tsc",
    "prerelease": "npm run build",
    "release": "standard-version -s -a"
  },
  "devDependencies": {
    "@types/cordova": "^0.0.34",
    "prettier": "^2.0.2",
    "replace": "^1.1.5",
    "standard-version": "^9.0.0",
    "tslint": "^6.1.1",
    "typescript": "^4.0.2"
  },
  "cordova": {
    "id": "cordova-plugin-detect-webview-engine",
    "platforms": [
      "ios"
    ]
  },
  "engines": {
    "cordovaDependencies": {
      "2.0.0": {
        "cordova": ">= 9.0.0",
        "cordova-ios": ">=5.0.0"
      },
      "1.0.1": {
        "cordova": ">= 6.4.0",
        "cordova-ios": ">=4.3.0"
      }
    }
  },
  "keywords": [
    "cordova",
    "cordova-plugin",
    "ecosystem:cordova",
    "cordova-ios",
    "engine",
    "webview",
    "wkwebview",
    "uiwebview"
  ],
  "funding": "https://github.com/sponsors/timbru31"
}
